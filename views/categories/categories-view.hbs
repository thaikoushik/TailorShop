<h1>Category View</h1>

<div class="row">
    {{# each categoryList }}




    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="{{ this.imagePath }}" class="img-responsive" alt="...">
            <div class="caption">
                <h3>{{ this.name }}</h3>

                <div class="clearfix">


                    <a href="/products-list/{{ this._id }}" class="btn btn-success pull-right" role="button">View Products</a>
                </div>
            </div>
        </div>
    </div>



    {{/each}}
</div>